{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^2.2",
        "arielmejiadev/larapex-charts": "^1.0",
        "awssat/discord-notification-channel": "^1.1",
        "beyondcode/laravel-server-timing": "^1.1",
        "driesvints/blade-icons": "^2.1",
        "dyrynda/laravel-efficient-uuid": "^4.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.4",
        "infinitypaul/laravel-password-history-validation": "^1.0",
        "laravel-frontend-presets/tailwindcss": "^4.0",
        "laravel/cashier": "^10.7",
        "laravel/framework": "^7.0",
        "laravel/scout": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/fractal": "^0.19.2",
        "mtolhuys/laravel-schematics": "^0.9.9",
        "robertogallea/eloquent-api": "^1.0",
        "sarfraznawaz2005/composer-cost": "^1.1",
        "slim/psr7": "^1.0",
        "spatie/geocoder": "^3.7",
        "spatie/laravel-flash": "^1.5",
        "spatie/laravel-model-cleanup": "^2.5",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-searchable": "^1.6",
        "titasgailius/terminal": "^0.2.0",
        "torann/geoip": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "ergebnis/composer-normalize": "^2.3",
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9.1",
        "johnkary/phpunit-speedtrap": "^3.2",
        "laravel-shift/blueprint": "^1.5",
        "laravel/dusk": "^5.9",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:model -RW",
            "@php artisan debugbar:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
